    "todo-example-app": {
      "projectType": "application",
      "root": "apps/todo-example-app",
      "sourceRoot": "apps/todo-example-app/src",
      "prefix": "orcha",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/todo-example-app",
            "index": "apps/todo-example-app/src/index.html",
            "main": "apps/todo-example-app/src/main.ts",
            "polyfills": "apps/todo-example-app/src/polyfills.ts",
            "tsConfig": "apps/todo-example-app/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/todo-example-app/src/favicon.ico", "apps/todo-example-app/src/assets"],
            "styles": ["apps/todo-example-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/todo-example-app/src/environments/environment.ts",
                  "with": "apps/todo-example-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "todo-example-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "todo-example-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "todo-example-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/todo-example-app/src/**/*.ts", "apps/todo-example-app/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/todo-example-app"],
          "options": {
            "jestConfig": "apps/todo-example-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
